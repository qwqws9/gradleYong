<script type="text/template" class="_tpl_together_image_component">
<div class="module_detail_coordiset extend_home_styling">

    <div class="area_title _area_title">
        <em class="anchor" id="homestyle"></em>
        <h3 class="title_coordiset _title_coordiset"></h3>
        <p class="text_coordiset _text_coordiset"></p>
        <h3 class="area_title_coordiset _area_title_coordiset" style="user-select: auto;"><span class="inner" style="user-select: auto;">함께 사면 좋은 상품</span></h3>
    </div>

    <div class="coordiset_wrap">

        <div class="flag">
            <span class="blind">SET</span>
            <span class="page">
            <span class="_coordi_set_no current number_1"><span class="blind">1</span></span>
            <span class="divide"><span class="blind">/</span></span>
            <span class="total number_{=totalCount}"><span class="blind">{=totalCount}</span></span>
            </span>
        </div>

        <div class="_coordi_rolling_list coordiset_mask">

            <ul class="_list coordiset">

                {for ixCoordi:coordi in togetherImageSet}
                <li class="coordiset_list">
                    <div class="coordiset_image">
                        <div class="thumbnail">
                            {if layoutVersion == 'VERSION2'}
                                <img class="image" src="{=coordi.imageUrl}?type=f640" alt="코디셋 이미지">
                            {else}
                                <img class="image" src="{=coordi.imageUrl}?type=f500_500" alt="코디셋 이미지">
                            {/if}
                        </div>
                        <div class="area_anchor">
                            {for imageTagRegion in coordi.imageTagRegions}
                            {if 0 < imageTagRegion.axisRateX && imageTagRegion.axisRateX < 100 && 0 < imageTagRegion.axisRateY && imageTagRegion.axisRateY < 100}
                                <a href="#" class="N=a:with2.itemtag item_anchor _click(nmp.front.sellershop.product.show.relation_product.highlightTogetherImageProductItem({=imageTagRegion.channelProductNo})) _stopDefault" style="left: {=imageTagRegion.axisRateX}%; top: {=imageTagRegion.axisRateY}%;"><span class="blind">상품목록으로 이동</span></a>
                            {/if}
                            {/for}
                        </div>
                        <button type="button" aria-pressed="{=coordi.zzim}" class="button_all_zzim N=a:with2.fav _zzim_button _click(nmp.front.sellershop.product.show.relation_product.keepRelationProduct({=coordi.relationProductNo})) _stopDefault" data-relation-product-id="{=coordi.relationProductNo}"><span class="blind">전체 찜하기</span></button>
                    </div>
                    <div class="_coordiset_product_base coordiset_product" data-coordiset-no="{=coordi.relationProductNo}">
                        <ul class="coordi_item_set">
                            {for ixProduct:product in coordi.productItems}
                                {if product.requireAdultAuth}
                                    <li class="_coordiset_product_item coordi_item_list" data-productid="{=product.id}">
                                        <div class="thumbnail thumbnail_addition shield_thumb"></div>

                                        <p class="name">성인인증 후 확인 가능합니다.</p>
                                        <a class="link" href="/aseado/products/{=product.id}" target="_blank"><span class="blind">상품 링크</span></a>
                                        <input type="checkbox" id="checkCoordiset_adult1" class="checkbox_coordiset" disabled="">
                                        <label for="checkCoordiset_adult1" class="label_coordiset"><span class="blind">선택하기</span></label>
                                    </li>
                                {elseif product.requireYouthSafe}
                                    <li class="_coordiset_product_item coordi_item_list" data-productid="{=product.id}">
                                        <div class="thumbnail thumbnail_addition adult_thumb"></div>

                                        <p class="name">성인인증 후 확인 가능합니다.</p>
                                        <a class="link" href="/aseado/products/{=product.id}" target="_blank"><span class="blind">상품 링크</span></a>
                                        <input type="checkbox" id="checkCoordiset_adult2" class="checkbox_coordiset" disabled="">
                                        <label for="checkCoordiset_adult2" class="label_coordiset"><span class="blind">선택하기</span></label>
                                    </li>
                                {else}
                                    <!-- [D] 상단 'item_anchor' 클릭시 해당 상품목록에 is_highlight 클래스 추가 후 제거 -->
                                    <!-- [D] 품절일 때 .soldout 추가 -->
                                    <li class="_coordiset_product_item coordi_item_list {if product.productStatusType == 'OUTOFSTOCK'}soldout{/if}" data-productid="{=product.id}">
                                        <div class="thumbnail">
                                            <img class="image" src="{=product.representImageUrl}?type=f80_80" alt="코디셋 아이템 이미지">
                                            <!-- [D] 찜한 경우 aria-pressed="true" aria-label="찜하기 취소" / 찜하지 않은 경우 aria-pressed="false" aria-label="찜하기" -->
                                            {if product.zzim}
                                            {set _pressed=true}
                                            {else}
                                            {set _pressed=false}
                                            {/if}
                                            <a class="_click(nmp.front.sellershop.toggleKeep({=product.id})) _stopDefault _responsive_scrap_button button_keep N=a:with2.mylist" role="button" data-scrap-item-id="{=product.id}" aria-pressed="{=_pressed}" href="#찜하기"><span class="icon">찜하기</span></a>
                                        </div>
                                        <p class="name">{=product.name}</p>

                                        {if product.productStatusType == 'OUTOFSTOCK'}
                                            <span class="label_soldout"><span class="blind">SOLDOUT</span></span>
                                        {elseif product.requireSubscribe}
                                            <span class="label_info">스토어찜 전용상품</span>
                                        {else}
                                            <em class="price"><span class="number">{js nmp.utility.convertNumberFormat(=product.discountedPrice)}</span><span class="currency">원</span></em>
                                            {if product.discountPercentage >= 1}
                                            <span class="discount">{=product.discountPercentage}%</span>
                                            {/if}
                                        {/if}

                                        {if curProductId == product.id}
                                        <span class="label_info">이 페이지 상품</span>
                                        <a class="_click(nmp.front.sellershop.product.show.relation_product.goScrollTop()) _stopDefault link N=a:with2.prd" href="#" target="_blank"><span class="blind">상품 링크</span></a>
                                        {else}
                                        <a class="link N=a:with2.prd" href="/aseado/products/{=product.id}" target="_blank"><span class="blind">상품 링크</span></a>
                                        {/if}

                                        {if product.requireSubscribe}
                                        <label class="_click(nmp.front.sellershop.product.show.relation_product.selectProductRequireSubscribe()) label_coordiset N=a:with2.one"><span class="blind">선택하기</span></label>
                                        {elseif product.productStatusType == 'OUTOFSTOCK'}
                                        <label class="label_coordiset"><span class="blind">선택하기</span></label>
                                        {else}
                                        <input type="checkbox" id="checkCoordiset_{=coordi.relationProductNo}_{=product.id}" class="_click(nmp.front.sellershop.product.show.relation_product.selectTogetherImageSetProduct({=coordi.relationProductNo})) _coordi_prod_checkbox checkbox_coordiset" data-productid="{=product.id}" checked="true">
                                        <label for="checkCoordiset_{=coordi.relationProductNo}_{=product.id}" class="label_coordiset"><span class="blind">선택하기</span></label>
                                        {/if}
                                    </li>
                                {/if}
                            {/for}
                        </ul>

                        <div class="total_price">
                            <span class="desc">총 상품금액</span>
                            <span class="_count count">({=coordi.productItems.length} ITEMS)</span>
                            <span class="price"><span class="_currency currency">0</span>원</span>
                        </div>
                        <button class="_click(nmp.front.sellershop.product.show.relation_product.showCoordiBridgeCartLayer()) _stopDefault all_check N=a:with2.cart"><span class="blind">선택상품 한번에 담기</span></button>
                    </div>
                </li>
                {/for}
            </ul>
        </div>
    </div>
    {if togetherImageSet.length > 1}
    <div class="area_coordi_pagination">
        <button class="_click(nmp.front.sellershop.product.show.relation_product.togetherImageRollingMovePrevious()) _stopDefault button_page past N=a:with2.btmprev"><span class="blind">이전 목록으로</span></button>
        <div class="_coordi_page_list coordi_pagination">
            {for ixCoordi:coordi in togetherImageSet}
            <button class="_click(nmp.front.sellershop.product.show.relation_product.togetherImageRollingMoveTo({=ixCoordi})) _stopDefault _coordi_page button_coordi_pagination N=a:with2.indicator" aria-selected="{if ixCoordi == 0}true{else}false{/if}"><span class="blind">{=ixCoordi+1} 페이지</span></button>
            {/for}
        </div>
        <button class="_click(nmp.front.sellershop.product.show.relation_product.togetherImageRollingMoveNext()) _stopDefault button_page next N=a:with2.btmnext"><span class="blind">다음 목록으로</span></button>
    </div>
    {/if}
</div>
</script>

